#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[n], unique[100];
    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    int k = 0;

    for (int i = 0; i < n; i++) {
        bool isUnique = true;

        for (int j = 0; j < k; j++) {
            if (arr[i] == unique[j]) {
                isUnique = false;
                break;
            }
        }

        if (isUnique)
            unique[k++] = arr[i];
    }

    cout << "Array with unique elements:\n";
    for (int i = 0; i < k; i++)
        cout << unique[i] << " ";

    return 0;
}

