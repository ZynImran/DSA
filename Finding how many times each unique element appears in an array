#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[n];
    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    cout << "Element Frequency\n";

    for (int i = 0; i < n; i++) {
        int count = 1;
        bool visited = false;

        for (int j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                visited = true;
                break;
            }
        }
        if (visited)
            continue;
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j])
                count++;
        }

        cout << arr[i] << " -> " << count << endl;
    }
    return 0;
}

